<%- include ('partials/header');%>

<h1>THIS IS INDEX!!</h1>

<% blogs.forEach((blog) => { %>
    <div>
        <h2><%= blog.title %></h2>
        <img src="<%= blog.image%>">
        <span><%= blog.created%></span>
        <p><%= blog.body.substr(0, 100) %>...</p>
        <a href="/blogs/<%= blog._id%>">Read More</a>
    </div>
<% }) %>

<%- include ('partials/footer');%>
